RewriteEngine on
RewriteRule ^categoria_([0-9].*)_.*$ index.php?categoria=$1 [L]
RewriteRule ^(comprar|vender):(.+)$ index.php?peticion=$1&op=$2 [L]
RewriteRule ^(iniciar|finalizar|registrar|perfil|vender|comprar|buscar|ayuda|ayuda|vip)$ index.php?peticion=$1 [L]
RewriteRule ^registro_(.+)_existe:(.*)$ PHP/traductor.php?peticion=registro_usuarios_$1&op=$2 [L]
